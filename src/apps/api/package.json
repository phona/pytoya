{
  "name": "@pytoya/api",
  "private": true,
  "version": "0.0.0",
  "description": "NestJS API service",
  "main": "dist/main.js",
  "scripts": {
    "start:dev": "dotenv -e .env.local -- nest start --watch",
    "start:debug": "dotenv -e .env.local -- nest start --debug --watch",
    "start:prod": "node dist/main",
    "cli": "dotenv -e .env.local -- node dist/cli",
    "cli:dev": "dotenv -e .env.local -- ts-node src/cli.ts",
    "build": "nest build",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:di": "jest --testPathPattern \"\\\\.di\\\\.spec\\\\.ts$\"",
    "migration:generate": "dotenv -e .env.local -- typeorm-ts-node-commonjs migration:generate -d src/database/data-source.ts",
    "migration:run": "dotenv -e .env.local -- typeorm-ts-node-commonjs migration:run -d src/database/data-source.ts",
    "migration:revert": "dotenv -e .env.local -- typeorm-ts-node-commonjs migration:revert -d src/database/data-source.ts",
    "schema:drop": "dotenv -e .env.local -- typeorm schema:drop"
  },
  "dependencies": {
    "@isaacs/brace-expansion": "^5.0.0",
    "@nestjs/axios": "4.0.1",
    "@nestjs/bullmq": "11.0.4",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "11.1.12",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.12",
    "@nestjs/jwt": "11.0.2",
    "@nestjs/mapped-types": "2.1.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.1.12",
    "@nestjs/platform-socket.io": "11.1.12",
    "@nestjs/swagger": "11.2.5",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/typeorm": "11.0.0",
    "@nestjs/websockets": "11.1.12",
    "ajv": "^8.12.0",
    "axios": "1.6.8",
    "bcrypt": "5.1.1",
    "bullmq": "5.13.0",
    "cache-manager": "^7.2.8",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "exceljs": "^4.4.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "ioredis": "5.4.1",
    "js-yaml": "4.1.1",
    "multer": "1.4.5-lts.1",
    "nest-commander": "3.20.1",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pdf-to-img": "^5.0.0",
    "pg": "8.11.5",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.16",
    "@nestjs/schematics": "11.0.9",
    "@nestjs/testing": "11.1.12",
    "@types/bcrypt": "5.0.2",
    "@types/express": "4.17.21",
    "@types/handlebars": "^4.0.40",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash.merge": "^4.6.9",
    "@types/multer": "1.4.11",
    "@types/node": "20.11.30",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^11.0.0",
    "jest": "^29.7.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  }
}

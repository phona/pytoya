{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@pytoya/api/*": ["src/apps/api/src/*"],
      "@pytoya/web/*": ["src/apps/web/*"],
      "@pytoya/common/*": ["src/apps/common/src/*"]
    }
  }
}
